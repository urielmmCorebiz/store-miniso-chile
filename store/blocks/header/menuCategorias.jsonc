    "props": {
      "width": "100%",
      "blockClass": "submenu-licencias"
    }
  },
  "flex-layout.row#categorias": {
    "children": [
      "flex-layout.col#categorias-1",
      "flex-layout.col#categorias-2"
    ],
    "props": {
      "fullWidth": true,
      "blockClass": "menu-categorias"
    }
  },
  "flex-layout.col#categorias-1": {
    "children": [
      "flex-layout.row#categorias-1",
      "flex-layout.row#categorias-2"
    ],
    "props": {
      "width": "78%",
      "blockClass": "categorias-1"
    }
  },
  "flex-layout.col#categorias-2": {
    "children": [
      "rich-text#categorias-imgTitle-01",
      "rich-text#categorias-imgTitle-02",
      "image#menucategorias-1"
    ],
    "props": {
      "width": "22%",
      "blockClass": "categorias-2"
    }
  },
  "rich-text#categorias-imgTitle-01": {
    "props": {
      "textAlignment": "CENTER",
