{
  "menu-item#menu-categorias": {
    "props": {
      "id": "menu-item-menu-categorias",
      "type": "custom",
      "iconId": null,
      "highlight": false,
      "itemProps": {
        "type": "internal",
        "href": "#",
        "tagTitle": "Categorias",
        "text": "Categorías"
      },
      "blockClass": "menu-title"
    },
    "blocks": ["submenu#menu-categorias"]
  },
  "submenu#menu-categorias": {
    "children": ["flex-layout.row#categorias"],
